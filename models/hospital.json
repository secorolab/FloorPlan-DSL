{
    "@context":[
        "http://robot-schema.org/context/geometry.context.json",
        "http://robot-schema.org/context/polytope.context.json",
        "http://robot-schema.org/context/floorplan.context.json"
    ],
    "@graph":[
        {
            "@id":"reception-frame-origin",
            "@type":"geom:Point"
        },
        {
            "@id":"reception-frame",
            "@type":"geom:Frame",
            "geom:origin":"reception-frame-origin"
        },
        {
            "@id":"reception-wall-0-frame-origin",
            "@type":"geom:Point"
        },
        {
            "@id":"reception-wall-1-frame-origin",
            "@type":"geom:Point"
        },
        {
            "@id":"reception-wall-2-frame-origin",
            "@type":"geom:Point"
        },
        {
            "@id":"reception-wall-3-frame-origin",
            "@type":"geom:Point"
        },
        {
            "@id":"reception-wall-0-frame",
            "@type":"geom:Frame",
            "geom:origin":"reception-wall-0-frame-origin"
        },
        {
            "@id":"reception-wall-1-frame",
            "@type":"geom:Frame",
            "geom:origin":"reception-wall-1-frame-origin"
        },
        {
            "@id":"reception-wall-2-frame",
            "@type":"geom:Frame",
            "geom:origin":"reception-wall-2-frame-origin"
        },
        {
            "@id":"reception-wall-3-frame",
            "@type":"geom:Frame",
            "geom:origin":"reception-wall-3-frame-origin"
        },
        {
            "@id":"reception-wall-0",
            "@type":"floor:Wall",
            "geom:Frame":"reception-wall-0-frame"
        },
        {
            "@id":"reception-wall-1",
            "@type":"floor:Wall",
            "geom:Frame":"reception-wall-0-frame"
        },
        {
            "@id":"reception-wall-2",
            "@type":"floor:Wall",
            "geom:Frame":"reception-wall-0-frame"
        },
        {
            "@id":"reception-wall-3",
            "@type":"floor:Wall",
            "geom:Frame":"reception-wall-0-frame"
        },
        {
            "@id":"reception-polytope",
            "@type":"poly:PolytopeWithFrame",
            "geom:Frame":"reception-frame",
            "poly:Points": [
                [-5, 5, 0],
                [5, 5, 0],
                [5, -5, 0],
                [-5, -5, 0]
            ]
        },
        {
            "@id":"reception-space",
            "@type":"floor:Space",
            "poly:PolytopeWithFrame":"reception-polytope",
            "floor:Walls": [
                "reception-wall-0",
                "reception-wall-1",
                "reception-wall-2",
                "reception-wall-3"
            ]
        }
    ]
}