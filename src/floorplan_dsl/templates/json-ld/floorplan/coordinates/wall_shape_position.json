{% extends "./geometry/coordinate/position.json" %}
{% import "./floorplan/identifiers.json" as ids %}

{% block coord_id -%}
{{ ids.coord_position_corner_id(poly_id, space, w) }}
{%- endblock %}

{% block of_position -%}
{{ ids.corner_position_id(poly_id, space, w) }}
{%- endblock %}

{% block wrt -%}
{{ ids.wall_point_id(space, w) }}
{%- endblock %}

{% block as_seen_by -%}
    {{ ids.wall_frame_id(w) }}
{%- endblock %}

{% block coords %}
"x": {{ point[0] }},
"y": {{ point[1] }}
{% endblock %}
